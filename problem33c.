#include <stdio.h>

int main() {
    int sum = 0; // Variable to hold the sum

    // Loop from 1 to 15
    for (int i = 1; i <= 15; i++) {
        sum += i; // Add the current number to the sum
    }

    // Print the result
    printf("The sum of the numbers between 1 and 15 inclusive is: %d\n", sum);

    return 0;
}
